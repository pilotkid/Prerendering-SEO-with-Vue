{
    "routes": [
        {
            "route": "/screenshots/*.{png,jpg,gif,webp}",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "/ScreenShots/*.{png,jpg,gif,webp}",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "/*.{png,jpg,gif,webp,pdf}",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "/js/chunk-vendors.*.js",
            "headers": {
                "Cache-Control": "public, max-age=2419200"
            }
        },
        {
            "route": "/dynamic",
            "rewrite": "/dynamic/index.html"
        },
        {
            "route": "/noindex",
            "rewrite": "/noindex/index.html"
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*"
        ]
    }
}